{
  "name": "huanlphp/huanl",
  "description": "this a php framework",
  "homepage": "https://github.com/huanl-php",
  "support": {
    "issues": "https://github.com/huanl-php/huanl/issues"
  },
  "authors": [
    {
      "name": "CodFrm",
      "email": "code.farmer@qq.com",
      "homepage": "https://blog.icodef.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "huanlphp/core": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1",
    "eaglewu/swoole-ide-helper": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.org"
    }
  }
}
